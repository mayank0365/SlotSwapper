{
  "name": "slotswapper",
  "version": "1.0.0",
  "description": "Peer-to-peer time-slot swapping platform - MERN stack",
  "scripts": {
    "install-all": "cd server && npm install && cd ../client && npm install",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "mern",
    "slot",
    "swap",
    "calendar",
    "time-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
